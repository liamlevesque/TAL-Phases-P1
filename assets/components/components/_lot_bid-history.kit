<div class="o-data-table h-mt-200">
    <div class="o-data-table--header">Bid History</div>
    <ul class="o-data-table--body">
        <li class="o-data-table--row g-wrapper" v-for="bid in lot.bids">
            <div class="g-cell">{{ bid.bidder }}</div>
            <div class="g-cell CCY">{{ bid.bid }}</div>
            <div class="g-cell g-cell__half">{{ bid.time | calendarTime }}</div>
        </li>
    </ul>
</div>